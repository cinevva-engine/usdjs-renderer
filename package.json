{
  "name": "@cinevva/usdjs-renderer",
  "version": "0.1.0",
  "type": "module",
  "description": "Headless Chromium renderer for @cinevva/usdjs-viewer output (pixel-parity screenshots).",
  "license": "MIT",
  "author": "Cinevva",
  "homepage": "https://cinevva-engine.github.io/usdjs-renderer/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cinevva-engine/usdjs-renderer.git"
  },
  "bugs": {
    "url": "https://github.com/cinevva-engine/usdjs-renderer/issues"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "usdjs-render-png": "./dist/render-usda-to-png.js",
    "usdjs-compare-ftlab-samples": "./dist/compare-ftlab-samples.js",
    "usdjs-generate-ftlab-gallery": "./dist/generate-ftlab-gallery.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "npm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitepress": "^1.5.0"
  },
  "peerDependencies": {
    "playwright": "^1.40.0"
  },
  "peerDependenciesMeta": {
    "playwright": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}

